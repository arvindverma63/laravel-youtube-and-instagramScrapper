{
    "openapi": "3.0.0",
    "info": {
        "title": "YouTube API Scraper",
        "description": "This API scrapes YouTube channel and video data using the Google YouTube Data API.",
        "contact": {
            "name": "API BackendCoders Team",
            "email": "support@yourdomain.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/search-youtube": {
            "post": {
                "tags": [
                    "YouTube"
                ],
                "summary": "Search YouTube channel data by username or URL",
                "description": "Fetches YouTube channel details and top videos based on a provided username or channel URL.",
                "operationId": "searchYoutubeApi",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "channel"
                                ],
                                "properties": {
                                    "channel": {
                                        "description": "YouTube channel username (e.g., @natgeo) or URL (e.g., https://www.youtube.com/@natgeo or https://www.youtube.com/channel/UCpI...)",
                                        "type": "string",
                                        "example": "@natgeo or https://www.youtube.com/@natgeo"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "YouTube channel data retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "channelQuery": {
                                            "type": "string",
                                            "example": "@natgeo"
                                        },
                                        "channelData": {
                                            "properties": {
                                                "title": {
                                                    "type": "string",
                                                    "example": "National Geographic"
                                                },
                                                "description": {
                                                    "type": "string",
                                                    "example": "Exploring the world..."
                                                },
                                                "subscribers": {
                                                    "type": "string",
                                                    "example": "1000000"
                                                },
                                                "videoCount": {
                                                    "type": "integer",
                                                    "example": 500
                                                },
                                                "viewCount": {
                                                    "type": "integer",
                                                    "example": 100000000
                                                },
                                                "category": {
                                                    "type": "string",
                                                    "example": "Education"
                                                },
                                                "banner": {
                                                    "type": "string",
                                                    "example": "https://..."
                                                },
                                                "thumbnail": {
                                                    "type": "string",
                                                    "example": "https://..."
                                                },
                                                "publishedAt": {
                                                    "type": "string",
                                                    "example": "2005-11-15T12:00:00Z"
                                                },
                                                "customUrl": {
                                                    "type": "string",
                                                    "example": "@natgeo"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "videos": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "title": {
                                                        "type": "string",
                                                        "example": "Amazing Wildlife"
                                                    },
                                                    "videoId": {
                                                        "type": "string",
                                                        "example": "dQw4w9WgXcQ"
                                                    },
                                                    "thumbnail": {
                                                        "type": "string",
                                                        "example": "https://..."
                                                    },
                                                    "views": {
                                                        "type": "integer",
                                                        "example": 100000
                                                    },
                                                    "publishedAt": {
                                                        "type": "string",
                                                        "example": "2023-01-01T12:00:00Z"
                                                    },
                                                    "description": {
                                                        "type": "string",
                                                        "example": "A documentary..."
                                                    },
                                                    "likes": {
                                                        "type": "integer",
                                                        "example": 5000
                                                    },
                                                    "comments": {
                                                        "type": "integer",
                                                        "example": 200
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error"
                    },
                    "500": {
                        "description": "YouTube API error"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "YouTube",
            "description": "YouTube"
        }
    ]
}